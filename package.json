{
  "name": "@sequencemedia/react-redux-render",
  "version": "1.0.14",
  "description": "React Render",
  "keywords": [
    "React",
    "Redux",
    "isomorphic",
    "render",
    "renderToString",
    "renderToStaticMarkup"
  ],
  "main": "./index.js",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "jonathanperry@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sequencemedia/react-redux-render.git"
  },
  "scripts": {
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "pre-commit": "npm run lint && npm run build && git add lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@hapi/boom": "^9.1.0",
    "debug": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "core-js": "^3.6.5",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
